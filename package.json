    "babel-preset-meteor": "^6.4.4",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "1.x.x",
    "eslint": "1.10.x",
    "eslint-plugin-react": "3.15.x",
    "mocha": "2.x.x",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "1.17.x"
  },
  "dependencies": {
    "domready": "^1.0.8",
    "externalify": "^0.1.0",
    "mantra-core": "^1.2.0",
    "material-ui": "^0.14.4",
