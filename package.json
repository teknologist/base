{
  "name": "asteroid",
  "version": "0.2.0-1",
  "scripts": {
    "lint": "eslint ./libs ./client ./server --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha client/**/tests/**/*.js --compilers js:babel-core/register",
